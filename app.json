{
    "name": "Rudra Userbot",
    "description": "An first Telegram Userbot with multi Clients for Spamming or Mass.",
    "keywords": ["pyrogram", "userbot"],
    "repository": "https://github.com/RUDRA-JAAT/Rudra-UserBot",
  "stack": "container",
    "env": {
        "STRING_SESSION1": {
            "description": "Fill there an string session",
            "required": true
        },
        "MONGO_URL": {
            "description": "Fill there an mongo database from mongodb.com",
            "required": true,
            "value": ""
        },
        "SUDO_USERS": {
            "description": "Fill there your I'd ",
            "required": true,
            "value": "1234567890 1234567890"
        },
        "OWNER_ID": {
            "description": "Fill there your I'd ",
            "required": true,
            "value": "1234567890"
        },
        "BOT_TOKEN": {
            "description": "fill with your bot token get from @BotFather on telegram",
            "required": true,
            "value": ""
        }

    }
  }
